useful_function <- function(x, y){ #argument x and y
if(x == y){
return("Exactly the same.")
}else if( sort(x) == sort(y)){
return("The same")
}else if ( x!=y){         #sort(x) !== sort(y)
return("Different")
}
}
useful_function(10, 5+5)
useful_function(c(1,2), c(2,1))
